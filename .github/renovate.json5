{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    "docker:enableMajor",
    "helpers:pinGitHubActionDigests",
    "replacements:k8s-registry-move",
    ":automergeBranch",
    ":automergeDigest",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":gitSignOff",
    ":semanticCommits",
    ":skipStatusChecks",
    ":timezone(Asia/Jakarta)",
    "github>monosense25/prod-cluster//.github/renovate/allowedVersions.json5",
    "github>monosense25/prod-cluster//.github/renovate/autoMerge.json5",
    "github>monosense25/prod-cluster//.github/renovate/commitMessage.json5",
    "github>monosense25/prod-cluster//.github/renovate/customManagers.json5",
    "github>monosense25/prod-cluster//.github/renovate/grafanaDashboards.json5",
    "github>monosense25/prod-cluster//.github/renovate/groups.json5",
    "github>monosense25/prod-cluster//.github/renovate/labels.json5",
    "github>monosense25/prod-cluster//.github/renovate/packageRules.json5",
    "github>monosense25/prod-cluster//.github/renovate/semanticCommits.json5",
  ],
  dependencyDashboardTitle: "Renovate Dashboard ðŸ¤–",
  suppressNotifications: ["prEditedNotification", "prIgnoreNotification"],
  rebaseWhen: "conflicted",
  assigneesFromCodeOwners: true,
  reviewersFromCodeOwners: true,
  ignorePaths: ["**/*.sops.*", "**/configs/**"],
  flux: {
    fileMatch: ["(^|/)kubernetes/.+\\.ya?ml$"],
  },
  "helm-values": {
    fileMatch: ["(^|/)kubernetes/.+\\.ya?ml$"],
  },
  kubernetes: {
    fileMatch: [
      "(^|/).taskfiles/.+\\.ya?ml$",
      "(^|/)talos/.+\\.ya?ml$",
      "(^|/)kubernetes/.+\\.ya?ml$",
    ],
  },
}